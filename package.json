{
  "name": "react-painlessform",
  "version": "0.0.2-alpha5",
  "description": "React Forms without pain",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "src",
    "lib",
    "lib/index.d.ts"
  ],
  "scripts": {
    "publish:alpha": "npm run build & npm publish -tag alpha",
    "publish:latest": "npm run build & npm publish",
    "build:commonjs": "tsc",
    "build:es": "tsc -p tsconfig.es.json",
    "build": "npm run build:es && npm run build:commonjs",
    "test:components": "mocha --require tests/setup.js -r ts-node/register tests/components/**/*.spec.tsx",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts && npm run test:components"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wroud/react-painlessform.git"
  },
  "keywords": [
    "typescript",
    "react",
    "form"
  ],
  "author": "Alexey Potsetsuev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wroud/react-painlessform/issues"
  },
  "homepage": "https://github.com/Wroud/react-painlessform#readme",
  "dependencies": {
    "react": "^16.3.0-0",
    "react-dom": "^16.0.0-0",
    "shallowequal": "^1.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/chai-arrays": "^1.0.2",
    "@types/enzyme": "^3.1.10",
    "@types/jsdom": "^11.0.4",
    "@types/mocha": "^5.0.0",
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.5",
    "@types/react-test-renderer": "^16.0.1",
    "@types/shallowequal": "^0.2.1",
    "@types/yup": "^0.24.3",
    "chai": "^4.1.2",
    "chai-arrays": "^2.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jsdom": "^11.8.0",
    "mocha": "^5.0.5",
    "react-test-renderer": "^16.3.2",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "2.8.3",
    "yup": "^0.24.1"
  }
}